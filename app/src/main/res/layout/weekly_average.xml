<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="wrap_content"
  android:orientation="horizontal"
  android:layout_marginLeft="@dimen/average_weekly_data_view_margin"
  android:layout_marginRight="@dimen/average_weekly_data_view_margin"
  android:gravity="center"
  android:background="@color/white">
<LinearLayout
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:orientation="vertical">

  <TextView
    android:id="@+id/tv_sevenDaysAgo"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:text="@string/placeholder_for_name_of_day_for_weekly_average_data_view"
    android:textSize="12sp" />
  <View
    android:id="@+id/rectangle_1"
    android:layout_width="@dimen/rectangle_for_weekly_data_view_width"
    android:layout_height="@dimen/rectangle_for_weekly_data_view_height"
    android:alpha="0.4"
    android:layout_marginRight="2dp"
    android:layout_marginLeft="2dp"
    android:layout_marginTop="5dp"/>
  <TextView
    android:id="@+id/valueForMonday"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:textSize="15sp"
    android:layout_marginTop="5dp"
    android:text="@string/data_not_available" />

</LinearLayout>
  <LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
      android:id="@+id/tv_sixDaysAgo"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_gravity="center"
      android:text="@string/placeholder_for_name_of_day_for_weekly_average_data_view"
      android:textSize="12sp" />
    <View
      android:id="@+id/rectangle_2"
      android:layout_width="@dimen/rectangle_for_weekly_data_view_width"
      android:layout_height="@dimen/rectangle_for_weekly_data_view_height"
      android:alpha="0.4"
      android:layout_marginRight="2dp"
      android:layout_marginLeft="2dp"
      android:layout_marginTop="5dp"/>
    <TextView
      android:id="@+id/valueForTuesday"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_gravity="center"
      android:textSize="15sp"
      android:layout_marginTop="5dp"
      android:text="@string/data_not_available" />
  </LinearLayout>
  <LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    >

    <TextView
      android:id="@+id/tv_FiveDaysAgo"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_gravity="center"
      android:text="@string/placeholder_for_name_of_day_for_weekly_average_data_view"
      android:textSize="12sp" />

    <View
      android:id="@+id/rectangle_3"
      android:layout_width="@dimen/rectangle_for_weekly_data_view_width"
      android:layout_height="@dimen/rectangle_for_weekly_data_view_height"
      android:layout_marginLeft="2dp"
      android:layout_marginRight="2dp"
      android:layout_marginTop="5dp"
      android:alpha="0.4" />
    <TextView
      android:id="@+id/valueForWednesday"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_gravity="center"
      android:textSize="15sp"
      android:layout_marginTop="5dp"
      android:text="@string/data_not_available" />

  </LinearLayout>
  <LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
      android:id="@+id/tv_FourDaysAgo"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_gravity="center"
      android:text="@string/placeholder_for_name_of_day_for_weekly_average_data_view"
      android:textSize="12sp" />

    <View
