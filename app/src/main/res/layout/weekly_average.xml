      android:id="@+id/valueForFriday"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_gravity="center"
      android:textSize="15sp"
      android:layout_marginTop="5dp"
      android:text="@string/data_not_available" />

  </LinearLayout>
  <LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
      android:id="@+id/tv_TwoDaysAgo"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_gravity="center"
      android:text="@string/placeholder_for_name_of_day_for_weekly_average_data_view"
      android:textSize="12sp" />

    <View
      android:id="@+id/rectangle_6"
      android:layout_width="@dimen/rectangle_for_weekly_data_view_width"
      android:layout_height="@dimen/rectangle_for_weekly_data_view_height"
      android:layout_marginLeft="2dp"
      android:layout_marginRight="2dp"
      android:layout_marginTop="5dp"
      android:alpha="0.4" />
    <TextView
      android:id="@+id/valueForSaturday"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_gravity="center"
      android:textSize="15sp"
      android:layout_marginTop="5dp"
      android:text="@string/data_not_available" />

  </LinearLayout>
  <LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
      android:id="@+id/tv_OneDayAgo"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_gravity="center"
      android:text="@string/placeholder_for_name_of_day_for_weekly_average_data_view"
      android:textSize="12sp" />

    <View
      android:id="@+id/rectangle_7"
      android:layout_width="@dimen/rectangle_for_weekly_data_view_width"
      android:layout_height="@dimen/rectangle_for_weekly_data_view_height"
      android:layout_marginLeft="2dp"
